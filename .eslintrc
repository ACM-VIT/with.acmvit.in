{
    "env": {
      "es6": true
    },
    "rules": {
        "no-var": "error",
        "semi": "error",
        "indent": "error",
        "no-multi-spaces": "error",
        "space-in-parens": "error",
        "no-multiple-empty-lines": "error",
        "prefer-const": "error",
        "no-use-before-define": "error"
      },
      "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  }
  }
  